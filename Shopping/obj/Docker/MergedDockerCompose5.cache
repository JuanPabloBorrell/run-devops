name: dockercompose2547215390611282390
services:
  shopping.api:
    build:
      context: C:\Users\JOB\Desktop\Deploying .Net Microservices with K8s, AKS and Azure DevOps\run-devops\Shopping
      dockerfile: Shopping.API/Dockerfile
    depends_on:
      shopping.db:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DatabaseSettings:ConnectionString: mongodb://shoppingdb:27017
    image: shoppingapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        published: "8000"
        protocol: tcp
  shopping.client:
    build:
      context: C:\Users\JOB\Desktop\Deploying .Net Microservices with K8s, AKS and Azure DevOps\run-devops\Shopping
      dockerfile: Shopping.Client/Dockerfile
    depends_on:
      shopping.api:
        condition: service_started
        required: true
      shopping.db:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: shoppingclient
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        published: "8001"
        protocol: tcp
  shopping.db:
    container_name: shoppingdb
    image: mongo
    networks:
      default: null
    ports:
      - mode: ingress
        target: 27017
        published: "27014"
        protocol: tcp
    restart: always
    volumes:
      - type: bind
        source: /site
        target: /data/db
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose2547215390611282390_default